{
  "author": "valeria gerez",
  "name": "package.json",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "devDependencies": {
    "cypress": "^10.6.0",
    "cypress-cucumber-preprocessor": "^4.1.0",
    "eslint-plugin-cypress": "^2.11.3",
    "cucumber-html-reporter": "^5.4.0",
    "@types/node": "^20.1.1"
  },
  "dependencies": {
    "gherkin-lint": "^4.2.1"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/support/step_definitions/",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/report/cucumber_report"
    }
  },
  "scripts": {
    "gherkin-lint": "./node_modules/.bin/gherkin-lint -c .gherkin-lintrc.json",
    "lint": "eslint 'cypress/**/*.js'",
    "start": "cypress open",
    "sanity": "./node_modules/.bin/cypress run --spec 'cypress/e2e/sanity/**/*' -e TAGS=\"@SANITY and not @BUG and not @WIP\"",
    "report": "node ./cypress/report/report.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valegerez/valeria_gerez_teladoc_challenge.git"
  }
}
